{
    "compilerOptions": {
        "target":"ES6",
        "module": "ES6",
        "moduleResolution":"Node",
        "lib": ["DOM", "ES2021", "dom.iterable"],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": false,
        "strict":true,
        "strictNullChecks":true,
        "strictPropertyInitialization":true,
        "noEmitOnError": true,
    },
}